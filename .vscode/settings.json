{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.run": "onSave",
  "eslint.validate": ["html", "javascript", "javascriptreact"],
  "files.autoSave": "onFocusChange",
  "editor.rulers": [120],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 12,
  "window.zoomLevel": 1
}
